{
  "name": "kanti/json-to-class",
  "license": "GPL-3.0-or-later",
  "type": "library",
  "authors": [
    {
      "name": "Matthias Vogel",
      "email": "git@kanti.de"
    }
  ],
  "require": {
    "php": "~8.2.0 || ~8.3.0",
    "nette/php-generator": "^4.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.3.2",
    "pluswerk/grumphp-config": "^7.1",
    "symfony/var-dumper": "^7.1.4"
  },
  "autoload": {
    "psr-4": {
      "Kanti\\JsonToClass\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Kanti\\JsonToClass\\Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "ergebnis/composer-normalize": true,
      "phpro/grumphp": true,
      "phpstan/extension-installer": true,
      "pluswerk/grumphp-config": true
    }
  },
  "scripts": {
    "post-update-cmd": [
      "composer bump -D",
      "composer normalize"
    ],
    "test": [
      "phpunit tests"
    ]
  }
}
